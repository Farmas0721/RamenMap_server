<p id="notice"><%= notice %></p>

<h1>Ramen</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ramen.each do |raman| %>
      <tr>
        <td><%= link_to 'Show', raman %></td>
        <td><%= link_to 'Edit', edit_raman_path(raman) %></td>
        <td><%= link_to 'Destroy', raman, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Raman', new_raman_path %>
