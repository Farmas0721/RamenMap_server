<%= form_with(model: ramen_store, local: true) do |form| %>

    <div class="form-group row">
      <%= form.label :store_name, class: 'col-12 col-form-label' %>
      <div class="col-12 col-sm-10">
        <%= form.text_field :store_name, class: 'form-control' %>
      </div>
    </div>

    <div class="form-group row">
      <%= form.label :store_number, class: 'col-12 col-form-label' %>
      <div class="col-12 col-sm-10">
        <%= form.text_field :store_number, class: 'form-control' %>
      </div>
    </div>

    <div class="form-group row">
      <%= form.label :latitude, class: 'col-12 col-form-label' %>
      <div class="col-12 col-sm-10">
        <%= form.text_field :latitude, class: 'form-control' %>
      </div>
    </div>

    <div class="form-group row">
      <%= form.label :longitude, class: 'col-12 col-form-label' %>
      <div class="col-12 col-sm-10">
        <%= form.text_field :longitude, class: 'form-control' %>
      </div>
    </div>

    <div class="actions d-flex justify-content-center">
      <%= form.submit class: 'btn btn-primary' %>
    </div>
<% end %>
